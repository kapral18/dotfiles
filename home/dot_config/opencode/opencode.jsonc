{
  "$schema": "https://opencode.ai/config.json",
  "small_model": "openrouter/minimax/minimax-m2.1",
  "model": "openrouter/minimax/minimax-m2.1",
  "theme": "catppuccin",
  "share": "disabled",
  "autoupdate": false,
  "permission": {
    "edit": "allow",
    "bash": "allow",
    "webfetch": "allow",
  },
  "mcp": {
    "playwright": {
      "type": "local",
      "command": ["npx", "-y", "@playwright/mcp@latest"],
      "enabled": true,
    },
    "sequential-thinking": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "@modelcontextprotocol/server-sequential-thinking",
      ],
      "enabled": true,
    },
  },
}
