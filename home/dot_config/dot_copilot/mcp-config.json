{
  "mcpServers": {
    "playwright": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@playwright/mcp@latest"],
      "env": {},
      "tools": ["*"]
    },
    "sequentialthinking": {
      "type": "stdio",
      "command": "docker",
      "args": [
        "run",
        "--pull=always",
        "--rm",
        "-i",
        "--init",
        "mcp/sequentialthinking"
      ],
      "env": {},
      "tools": ["*"]
    }
  }
}
